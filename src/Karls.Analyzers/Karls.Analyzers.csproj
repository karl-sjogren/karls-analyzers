    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
	<LangVersion>latest</LangVersion>

	<Authors>Karl-Johan Sj√∂gren</Authors>
	<RepositoryUrl>https://github.com/karl-sjogren/karls-analyzers/</RepositoryUrl>
